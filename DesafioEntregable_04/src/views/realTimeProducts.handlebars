<h1>Productos disponibles (en tiempo real):</h1>
<form id = "Productform"> 
    <ul>
        <li>
            <label for = "productName">Name:</label>
            <input type = "text" id = "productName" name = "productName" >
        </li>
        <li>
            <label for = "price">Price:</label>
            <input type = "number" id = "price" name = "price" >
        </li>
        <li>
            <label for = "code">Code:</label>
            <input type = "text" id = "code" name = "code" >
        </li>
        <li>
            <label for = "description">Description:</label>
            <input type = "text" id = "description" name = "description" >
        </li>
        <li>
            <label for = "stock">Stock:</label>
            <input type = "number" id = "stock" name = "stock" >
        </li>
        <li>
            <label for = "thumbnail">Thumbnail:</label>
            <input type = "text" id = "thumbnail" name = "thumbnail">
        </li>
        <li>
            <button type = "submit" id = "btt">Send</button>
        </li>
    </ul>
</form>

<ol id="products">
    {{#each products}}
        <li>
            <ul>
                <li>ID: {{this.id}}</li>
                <li>Nombre: {{this.title}}</li>
                <li>Precio: {{this.price}}</li>
                <li>Código: {{this.code}}</li>
                <li>Descripción: {{this.description}}</li>
                <li>Stock: {{this.stock}}</li>
            </ul>
            <br>
        </li>
    {{/each}}
</ol>

<script src="socket.io/socket.io.js"></script>
<script src="assets/scripts/realTimeProducts.js"></script>